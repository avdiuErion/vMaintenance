<div class="admin-title">
    <a href="/maintenances/byVId/<%= vehicleId %>">
        <button type="button" class="btn btn-primary">&larr;Kthehu</button>
    </a>
</div>

<div>
    <h2 style="padding-bottom: 1%; padding-top: 1%;">Shto servisim të ri</h2>
    <form action="/maintenances/<%= vehicleId %>" method="post">
        <div class="form-group row">
            <label for="colFormLabel" class="col-sm-2 col-form-label">Titulli</label>
            <div class="col-sm-6">
                <input type="text" class="form-control" id="colFormLabel" name="title">
            </div>
        </div>
        <div class="form-group row">
            <label for="colFormLabel" class="col-sm-2 col-form-label">Shënime</label>
            <div class="col-sm-6">
                <input type="text" class="form-control" id="colFormLabel" name="notes">
            </div>
        </div>
        <div class="form-group row">
            <label for="colFormLabel" class="col-sm-2 col-form-label">Pagesa</label>
            <div class="col-sm-6">
                <input type="text" class="form-control" id="colFormLabel" name="price">
            </div>
        </div>
        <div class="form-group row">
            <label for="colFormLabel" class="col-sm-2 col-form-label">Punëkryesi</label>
            <div class="col-sm-6">
                <input type="text" class="form-control" id="colFormLabel" name="contractor">
            </div>
        </div>
        <div class="form-group row">
            <label for="colFormLabel" class="col-sm-2 col-form-label">Kontakti i punëkryesit</label>
            <div class="col-sm-6">
                <input type="text" class="form-control" id="colFormLabel" name="contractorContact">
            </div>
        </div>
        <button type="submit" class="btn btn-success">Shto</button>
</div>
</form>
</div>